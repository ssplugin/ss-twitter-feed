{
    "name": "ssplugin/ss-twitter-feed",
    "description": "Show Recent Tweets.",
    "type": "craft-plugin",
    "version": "1.0.7",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ss twitter feed"
    ],
    "support": {
        "docs": "https://github.com/ssplugin/ss-twitter-feed/blob/master/README.md",
        "issues": "https://github.com/ssplugin/ss-twitter-feed"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SystemSeeders",
            "homepage": "http://www.systemseeders.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "ssplugin\\sstwitterfeed\\": "src/"
        }
    },
    "extra": {
        "name": "SS Twitter Feed",
        "handle": "ss-twitter-feed",
        "hasCpSettings": true,
        "hasCpSection": false,
        "documentationUrl": "https://github.com/ssplugin/ss-twitter-feed/blob/master/README.md",
        "changelogUrl": "https://github.com/ssplugin/ss-twitter-feed/blob/master/CHANGELOG.md",
        "components": {
            "ssTwitterFeedService": "ssplugin\\sstwitterfeed\\services\\SsTwitterFeedService"
        },
        "class": "ssplugin\\sstwitterfeed\\SsTwitterFeed"
    }
}
